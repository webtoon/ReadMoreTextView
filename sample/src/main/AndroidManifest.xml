<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.webtoonscorp.android.readmore.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.ReadMoreTextView">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.ViewSampleActivity"
            android:exported="false"
            android:label="@string/view_title" />
        <activity
            android:name=".compose.foundation.ComposeFoundationSampleActivity"
            android:exported="false" />
        <activity
            android:name=".compose.material.ComposeMaterialSampleActivity"
            android:exported="false" />
        <activity
            android:name=".compose.material3.ComposeMaterial3SampleActivity"
            android:exported="false"
            android:theme="@style/Theme.Material3.DayNight" />
    </application>

</manifest>
